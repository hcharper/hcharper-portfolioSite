{
  "name": "module-9-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests using Selenium WebDriver for the Blog Site",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles",
    "test:headless": "HEADLESS=true jest --runInBand --detectOpenHandles",
    "test:verbose": "jest --runInBand --verbose --detectOpenHandles"
  },
  "keywords": [
    "selenium",
    "e2e",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chromedriver": "^143.0.4",
    "jest": "^29.7.0",
    "selenium-webdriver": "^4.40.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 60000,
    "testMatch": [
      "**/*.e2e.test.js"
    ],
    "setupFilesAfterEnv": [
      "./setup.js"
    ]
  }
}
